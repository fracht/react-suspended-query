{
    "name": "react-suspended-query",
    "version": "0.2.2",
    "description": "React library to make queries using react suspenses and cache.",
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "types": "./dist/react-suspended-query.d.ts",
    "module": "./dist/react-suspended-query.esm.js",
    "scripts": {
        "build": "aqu build",
        "start": "aqu watch",
        "lint": "eslint src test",
        "lint:fix": "aqu lint --fix",
        "format": "prettier --write **/*.{ts,tsx,js,jsx,json,md,yml,yaml}",
        "test": "aqu test --passWithNoTests",
        "test:watch": "aqu test --passWithNoTests --watch",
        "test:coverage": "aqu test --coverage --json --silent --ci --outputFile=\"report.json\"",
        "test:log-coverage": "aqu test --coverage --silent --ci --coverageReporters=text",
        "release": "np",
        "prepublishOnly": "aqu build",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlexShukel/react-suspended-query.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AlexShukel/react-suspended-query/issues"
    },
    "homepage": "https://github.com/AlexShukel/react-suspended-query#readme",
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@esbuild-plugins/node-resolve": "^0.2.2",
        "@sirse-dev/eslint-config": "^1.0.12",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@types/jest": "^26.0.24",
        "@types/object-hash": "^2.2.1",
        "@types/react": "^18.0.14",
        "@types/testing-library__jest-dom": "^5.14.5",
        "aqu": "^0.3.2",
        "esbuild": "^0.17.8",
        "eslint": "^8.19.0",
        "np": "^7.6.1",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^4.7.4",
        "vitepress": "1.0.0-alpha.46"
    },
    "peerDependencies": {
        "react": ">=18"
    },
    "dependencies": {
        "@sirse-dev/safe-context": "^0.2.0",
        "object-hash": "^3.0.0",
        "tiny-invariant": "^1.2.0"
    },
    "pnpm": {
        "peerDependencyRules": {
            "ignoreMissing": [
                "@algolia/client-search"
            ]
        }
    }
}
